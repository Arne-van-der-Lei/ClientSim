{
	"metadata" : [
		{
			"src" : [
				{
					"files" : [ "**/*.cs" ],
					"exclude" : [ ],
					"src" : "../../Runtime"
				},
				{
					"files" : [ "**/*.cs" ],
					"exclude" : [ ],
					"src" : "../../Editor"
				}
			],
			"dest" : "classes",
			"disableGitFeatures" : false,
			"disableDefaultFilter" : true
		}
	],
	"build" : {
		"content" : [
			{
				"files" : [ "classes/**.yml", "classes/index.md"]
			},
			{
				"files" : [ "index.md", "toc.yml", "articles/**/*.md", "articles/**/toc.yml" ]
			}
		],
		"resource" : [
			{
				"files" : [ "images/**", "logo.png" ]
			}
		],
		"overwrite" : [
			{
				"files" : [ "classes/**.md" ],
				"exclude" : [ "obj/**", "_site/**" ]
			}
		],
		"dest" : "_site",
		"globalMetadataFiles" : [],
		"globalMetadata": {
			"_appLogoPath": "logo.png"
		},
		"fileMetadataFiles" : [],
		"template" : [ "default", "templates/vrc" ],
		"postProcessors" : [],
		"markdownEngineName" : "markdig",
		"noLangKeyword" : false,
		"keepFileLink" : false,
		"cleanupCacheHistory" : false,
		"disableGitFeatures" : true
	}
}